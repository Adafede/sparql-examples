prefix ex: <https://sparql.rhea-db.org/.well-known/sparql-examples/> 
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> 
ex:13
    a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    sh:prefixes ex: ;
    rdfs:comment """Select all the descendants of <a href="https://www.ebi.ac.uk/chebi/searchId.do?chebiId=CHEBI:35179">CHEBI:35179</a> (a 2-oxo carboxylate) in the ChEBI hierarchy (using rdfs:subClassOf+), used in Rhea reactions or not"""^^rdf:html ;
    sh:select """SELECT
  ?chebiDescendant  
  (STR(?label) AS ?chebiDescendantName) 
WHERE {
  ?chebiDescendant rdfs:subClassOf+ CHEBI:35179 .
  ?chebiDescendant rdfs:label ?label .
}
ORDER BY ?chebiDescendant""" .


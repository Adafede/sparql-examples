prefix ex: <https://sparql.orthodb.org/.well-known/sparql-examples/>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> 
ex:15
    a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    sh:prefixes _:sparql_examples_prefixes ;
    rdfs:comment """Find orthologous group/s at any level containing protein with Uniprot id P12345"""^^rdf:HTML ;
    sh:select """prefix : <http://purl.orthodb.org/>
select *
where {
?og a :OrthoGroup; :ogBuiltAt ?level; :hasMember/rdfs:seeAlso uniprot:P12345 
}""" .

prefix ex: <https://sparql.orthodb.org/.well-known/sparql-examples/>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> 
ex:2
    a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    sh:prefixes _:sparql_examples_prefixes ;
    rdfs:comment """Find all clades, i.e. taxonomic levels where orthologous groups were built on, with their Latin names and ranks underneath Bacteria, sorted alphabetically by rank, then name"""^^rdf:HTML ;
    sh:select """prefix : <http://purl.orthodb.org/>
select * where {
?taxon a :Clade
; rdfs:subClassOf+ taxon:2
; up:scientificName ?name
; up:rank ?rank
} order by ?rank ?name""" .

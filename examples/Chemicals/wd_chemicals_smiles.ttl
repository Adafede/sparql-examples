@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <https://adafede.github.io/sparql-examples/Chemicals/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix hint: <http://www.bigdata.com/queryHints#> .
@prefix wdt: <http://www.wikidata.org/prop/direct/> .

ex:wd_chemicals_smiles a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
rdfs:comment "Canonical SMILES"@en ;
sh:prefixes _:sparql_examples_prefixes ;
sh:select """
PREFIX hint: <http://www.bigdata.com/queryHints#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
#title: Canonical SMILES
SELECT ?item ?smiles_canonical WHERE {
    ?item wdt:P233 ?smiles_canonical. hint:Prior hint:rangeSafe true.
}
""";
schema:target <https://query.wikidata.org/sparql> ;
dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
schema:keywords "chemicals" ;
dc:creator "Adriano Rutz" .

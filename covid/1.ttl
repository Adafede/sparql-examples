prefix ex: <https://covid-19-sparql.expasy.org/.well-known/sparql-examples/> 
prefix up: <http://purl.uniprot.org/core/> 
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> 

ex:1
    a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    sh:prefixes ex: ;
    rdfs:comment """Get all steps in the pangenome build, with their start and end positions""" ;
    sh:select """SELECT 
    ?step 
    ?bp 
    ?ep 
    ?path
WHERE {?step a vg:Step; 
faldo:begin ?bp ; 
faldo:end ?ep . 
?bp a faldo:Position ; 
faldo:position ?begin .
?ep a faldo:Position ; 
faldo:reference ?path ;
faldo:position ?end .
}""" .

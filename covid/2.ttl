prefix ex: <https://covid-19-sparql.expasy.org/.well-known/sparql-examples/> 
prefix up: <http://purl.uniprot.org/core/> 
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> 

ex:2
    a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    sh:prefixes ex: ;
    rdfs:comment """Get UniProt proteins linked to a vg:Path in this graph""" ;
    sh:select """SELECT DISTINCT ?uniprot ?path WHERE { ?path a vg:Path . 
  ?embl up:locatedOn/owl:sameAs ?path .
  ?uniprot rdfs:seeAlso ?embl . }""" .
